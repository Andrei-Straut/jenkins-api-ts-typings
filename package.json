{
  "name": "jenkins-api-ts-typings",
  "version": "0.8.2",
  "description": "Jenkins JSON API TypeScript Typings",
  "keywords": [
    "jenkins",
    "typings",
    "typescript",
    "javascript",
    "jenkins typings",
    "jenkins api"
  ],
  "main": "dist/jenkins-api-ts-typings.umd.js",
  "module": "dist/jenkins-api-ts-typings.es5.js",
  "typings": "dist/types/jenkins-api-ts-typings.d.ts",
  "files": [
    "dist"
  ],
  "author": "Andrei Straut <andrei.straut@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrei-Straut/jenkins-api-ts-typings.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist && rimraf *.tgz",
    "build": "tsc && rollup -c && rimraf compiled && typedoc --out dist/docs --target es6 --theme minimal src && npm run git-info",
    "build-pkg": "npm run build && npm pack && npm run copy-files && npm run clean",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts'",
    "start": "tsc-watch --onSuccess \"rollup -c\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "clean": "rimraf *.tgz && rimraf test-report.*",
    "copy-files": "copyfiles package.json *.tgz test-report.* dist",
    "git-info": "git log -1 > dist/gitinfo"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --no-semi",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^20.0.0",
    "@types/node": "^8.0.0",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "copyfiles": "^1.2.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.0",
    "jest": "^20.0.4",
    "jest-junit-reporter": "^1.1.0",
    "lint-staged": "^4.0.0",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.4.4",
    "prompt": "^1.0.0",
    "replace-in-file": "^2.5.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "semantic-release": "^6.3.6",
    "ts-jest": "^20.0.6",
    "ts-node": "^3.0.6",
    "tsc-watch": "^1.0.5",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^6.0.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.4",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {}
}
